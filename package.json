{
  "name": "libromarks-server",
  "version": "0.0.1",
  "description": "GraphQL server to manage bookmarks stored by users",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "start": "cross-env NODE_ENV=development ts-node-dev --clear -r dotenv/config src/index.ts",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "prepare": "husky install",
    "eslint": "eslint --fix \"src/**\"",
    "knex:init": "knex init -x ts",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:down",
    "migrate:reset": "knex migrate:rollback --all && knex migrate:latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GerardoAGL96/libromarks-server.git"
  },
  "keywords": [
    "graphql",
    "quotes",
    "node",
    "express"
  ],
  "author": "Gerardo Garcia <gerardoagl96@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GerardoAGL96/libromarks-server/issues"
  },
  "homepage": "https://github.com/GerardoAGL96/libromarks-server#readme",
  "devDependencies": {
    "@jest/globals": "^26.6.2",
    "@tsconfig/node12": "^1.0.7",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.31",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.26.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "googleapis": "^73.0.0",
    "graphql": "^15.5.0",
    "knex": "^0.21.18",
    "mysql": "^2.18.1",
    "uuid": "^8.3.2"
  }
}
